{
  "compilerOptions": {
    /* Basic Options */
    "target": "es6",
    "sourceMap": true,
    "downlevelIteration": true,
    /* Strict Type-Checking Options */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  }
}
